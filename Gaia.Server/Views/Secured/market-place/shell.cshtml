                <div class="popup-header">
                    <span>Notifications</span>
                </div>
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>Description</th>
                            <th>Category</th>
                            <th class="text-center">Priority</th>
                        </tr>
                    </thead>
