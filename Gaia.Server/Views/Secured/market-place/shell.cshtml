                        <tr>
                            <td><span class="status status-success item-before"></span> <a href="#">Frontpage fixes</a></td>
                            <td><span class="text-smaller text-semibold">Bugs</span></td>
                            <td class="text-center"><span class="label label-success">87%</span></td>
                        </tr>
                        <tr>
                            <td><span class="status status-danger item-before"></span> <a href="#">CSS compilation</a></td>
                            <td><span class="text-smaller text-semibold">Bugs</span></td>
                            <td class="text-center"><span class="label label-danger">18%</span></td>
                        </tr>
                        <tr>
                            <td><span class="status status-info item-before"></span> <a href="#">Responsive layout changes</a></td>
                            <td><span class="text-smaller text-semibold">Layout</span></td>
                            <td class="text-center"><span class="label label-info">52%</span></td>
                        </tr>
                        <tr>
                            <td><span class="status status-success item-before"></span> <a href="#">Add categories filter</a></td>
                            <td><span class="text-smaller text-semibold">Content</span></td>
                            <td class="text-center"><span class="label label-success">100%</span></td>
                        </tr>
                        <tr>
                            <td><span class="status status-success item-before"></span> <a href="#">Media grid padding issue</a></td>
                            <td><span class="text-smaller text-semibold">Bugs</span></td>
                            <td class="text-center"><span class="label label-success">100%</span></td>
                        </tr>
                    </tbody>
                </table>
            </div>
                *@
            </li>
            <li class="user dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown">
                    <img src="~/Content/images/default-user.png" style="border-radius:50%;" alt="">
                    <span>@Model.UserId</span>
                    <i class="caret"></i>
                </a>
                <ul class="dropdown-menu dropdown-menu-right icons-right">
                    <li><a href ng-click="vm.logout()"><i class="icon-exit"></i> Logout</a></li>
                </ul>
            </li>
        </ul>
    </div>
    <!-- /navbar -->

    <!-- Page container -->
    <div class="page-container">

        <!-- Sidebar -->
        <div class="sidebar">
            <div class="sidebar-content">

                <!-- Main navigation -->
                <ul class="navigation">
                    <li class=""><a href="/view-server/secured/dashboard/shell"><span>Dashboard</span> <i class="icon-screen2"></i></a></li>
                </ul>

                <ul class="navigation">
                    <li class="">
                        <a href="/view-server/secured/gaia/shell"><span>Gaia</span> 
                            <i class="">
                                <img src="~/Content/images/GaiaLogo.svg" style="height:24px; width:24px;" />
                            </i>
                        </a>
                    </li>
                </ul>

                <ul class="navigation">
                    <li>
                        <a href="/view-server/secured/activities/shell"><span>Activities</span> <i class="icon-star4"></i></a>
                    </li>
                    <li>
                        <a href="/view-server/secured/community/shell"><span>Community</span> <i class="fa fa-users"></i></a>
                    </li>
                    <li class="active">
                        <a href><span>Market Place</span> <i class="fa fa-shopping-cart"></i></a>
                    </li>
                </ul>

                <ul class="navigation">
                    <li>
                        <a href="/view-server/secured/settings/shell"><span>Settings</span> <i class="fa fa-wrench"></i></a>
                    </li>
                </ul>
                <!-- /main navigation -->

            </div>
        </div>
        <!-- /sidebar -->

        <!-- Page content -->
        <div class="page-content" ng-controller="MarketPlaceViewModel as vm">

            <div class="page-header">
                <div class="page-title">
                    <h3>Market Place <small>Buy &amp; Sell products/services</small></h3>
                </div>

                <context-toolbar id="context-toolbar" class="col-md-8 col-sm-8 col-xs-12 "></context-toolbar>
            </div>

            <div class="tabbable page-tabs">
                <ul class="nav nav-tabs">
                    <li class="" ng-class="{active: vm.isCustomerActive()}">
                        <a ui-sref="customer" style="line-height:22px;">
                            <i class="icon-basket" style="font-size:20px;"></i> Buy
                        </a>
                    </li>
                    <li class="" ng-class="{active: vm.isMerchantActive()}" ng-show="vm.isMerchantAccount">
                        <a ui-sref="merchant" style="line-height:22px;">
                            <i style="width:22px; height:22px; line-height:22px" class="fa fa-money fa-2x"></i>&nbsp; Sell
                        </a>
                    </li>
                    <li class="pull-right" ng-class="{active: vm.isPreferencesActive()}">
                        <a ui-sref="configure" style="line-height:22px;">
                            <i class="icon-heart2" style="font-size:18px;"></i> My Preferences
                        </a>
                    </li>
                </ul>
                <div class="tab-content clearfix">
                    <div class="tab-pane active gaia-fade-in gaia-slide-in-left" ui-view></div>
                </div>
            </div>
            
            
            <!-- Footer -->
            <div class="footer clearfix" style="margin-top:20px;">
                <div class="pull-left"><b>&copy; &nbsp;@DateTime.Now.Year.</b> &nbsp; Powered By <a href="http://DevSoft.net.ng">DevSoft</a></div>
                <div class="pull-right icons-group">
                    <a href="#"><i class="icon-screen2"></i></a>
                    <a href="#"><i class="icon-balance"></i></a>
                    <a href="#"><i class="icon-cog3"></i></a>
                </div>
            </div>
            <!-- /footer -->

        </div>
        <!-- /page content -->
    </div>
    <!-- /content -->

    <div class="hidden" 
         id="local-models"
         simple-models="UserId:@Model.UserId;AccessProfiles:@Model.AccessProfiles.JoinUsing(",")"></div>


    <script type="text/javascript" src="~/app/utils/extensions.js"></script>
    <script type="text/javascript" src="~/app/utils/tools.js"></script>
    <script type="text/javascript" src="~/app/utils/constants.js"></script>
    <script type="text/javascript" src="~/app/utils/map.js"></script>
    <script type="text/javascript" src="~/app/models/axis-luna-domain.js"></script>
    <script type="text/javascript" src="~/app/models/axis-apollo-domain.js"></script>
    <script type="text/javascript" src="~/app/models/axis-pollux-domain.js"></script>
    <script type="text/javascript" src="~/app/models/gaia-domain.js"></script>
    <script type="text/javascript" src="~/app/directives/binaryData.js"></script>
    <script type="text/javascript" src="~/app/services/utility-services.js"></script>
    <script type="text/javascript" src="~/app/services/gaia-services.js"></script>
    <script type="text/javascript" src="~/app/services/pollux-services.js"></script>
    <script type="text/javascript" src="~/app/services/context-toolbar.js"></script>
    <script type="text/javascript" src="~/app/view-models/marketplace-viewmodels.js"></script>
    <script type="text/javascript" src="~/app/view-models/shared-viewmodels.js"></script>
    <script type="text/javascript" src="~/app/modules/marketplace-app.js"></script>
</body>
</html>