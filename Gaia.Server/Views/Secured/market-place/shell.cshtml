@using Axis.Luna.Extensions;
@model Gaia.Server.Controllers.MVC.Models.ShellModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1">

    <title>Gaia - Market Place</title>

    <link href="/themes/londinium/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/themes/londinium/css/londinium-theme.css" rel="stylesheet" type="text/css">
    <link href="/themes/londinium/css/styles.css" rel="stylesheet" type="text/css">
    <link href="/themes/londinium/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/fonts/cyrillic-ext.css" rel="stylesheet" type="text/css">

    <link href="~/Content/toastr.min.css" rel="stylesheet" type="text/css">
    <link href="~/scripts/summernote/summernote.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css">

    <link rel="stylesheet" href="~/Content/ring-loader.css" />
    <link rel="stylesheet" href="~/Content/cube-loader.css" />

    <link rel="stylesheet" href="~/Content/font-awesome/css/font-awesome.min.css" />


    <script type="text/javascript" src="~/scripts/jquery-1.10.1.min.js"></script>
    <script type="text/javascript" src="~/scripts/jquery-ui-1.10.1.min.js"></script>
    <script type="text/javascript" src="~/scripts/toastr.min.js"></script>
    <script type="text/javascript" src="~/scripts/summernote/summernote.min.js"></script>

    <script type="text/javascript" src="/themes/londinium/js/plugins/forms/uniform.min.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/plugins/forms/multiselect.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/plugins/interface/jgrowl.min.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/plugins/interface/daterangepicker.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/plugins/interface/moment.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/plugins/forms/tags.min.js"></script>

    <script type="text/javascript" src="~/themes/londinium/js/application_blank.js"></script>
    <script type="text/javascript" src="/themes/londinium/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="~/scripts/loaders.js"></script>

    <script type="text/javascript" src="~/scripts/moment.min.js"></script>

    <script type="text/javascript" src="~/scripts/angular.min.js"></script>
    <script type="text/javascript" src="~/scripts/angular-sanitize.min.js"></script>
    <script type="text/javascript" src="~/scripts/angular-animate.min.js"></script>
    <script type="text/javascript" src="~/scripts/angular-route.min.js"></script>
    <script type="text/javascript" src="~/scripts/angular-ui-router.min.js"></script>

</head>
<body class="navbar-fixed" ng-app="gaiaMarketPlace">
    <!-- Navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" ng-controller="NavbarViewModel as vm">
        <div class="navbar-header">
            <a class="navbar-brand" style="padding:1px;" href="#">
                <img src="~/Content/images/GaiaLogo.svg" style="height:50px; width:40px;" alt="Gaia">
                <span style="display:inline-block;margin-bottom:5px; vertical-align:bottom; font-family:jura; font-size:x-large">Gaia</span>
            </a>
            <a class="sidebar-toggle"><i class="icon-menu2"></i></a>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-icons">
                <span class="sr-only">Toggle navbar</span>
                <i class="icon-grid3"></i>
            </button>
            <button type="button" class="navbar-toggle offcanvas">
                <span class="sr-only">Toggle navigation</span>
                <i class="icon-menu2"></i>
            </button>
        </div>

        <ul class="nav navbar-nav navbar-right collapse" id="navbar-icons">
            <li class="">
                <a href="/view-server/secured/settings/shell">
                    <i class="fa fa-wrench"></i>
                </a>
            </li>
            <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle">
                    <i class="fa fa-bell"></i>
                </a>
                @*
